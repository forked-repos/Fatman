<!-- *****************************************************************************************************************
 PORTFOLIO SECTION
 ***************************************************************************************************************** -->
 <div id="portfoliowrap">
    <h3>FEATURE CATEGORIES</h3>

    <div class="portfolio-centered">
        <div class="recentitems portfolio">

        	<% for (var i = 0; i < subCategories.length; i++) { %>
				<div class="portfolio-item graphic-design">
					<div class="he-wrap tpl6">
						<img src="<%= subCategories[i].picture %>" alt="">
						<div class="he-view">
							<div class="bg a0" data-animate="fadeIn">
                            	<h3 class="a1" data-animate="fadeInDown"><%= subCategories[i].type %></h3>
                            	<a data-rel="prettyPhoto" href="<%= subCategories[i].picture %>" class="dmbutton a2" data-animate="fadeInUp"><i class="fa fa-search"></i></a>
                            	<a href="single-project.html" class="dmbutton a2" data-animate="fadeInUp"><i class="fa fa-link"></i></a>
                    		</div><!-- he bg -->
						</div><!-- he view -->		
					</div><!-- he wrap -->
				</div><!-- end col-12 -->  		
        	<% } %>
                
        </div><!-- portfolio -->
    </div><!-- portfolio container -->
 </div><!--/Portfoliowrap -->

